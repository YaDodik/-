#include <iostream>
#include <vector>

std::vector<int> increaseVector(std::vector<int> vec, int n) {
    for(int i = 0; i < vec.size(); i++) {
        vec[i] += n;
    }
    return vec;
}

int main() {
    std::vector<int> vec = {1, 2, 3, 4, 5};
    int n = 10;
    
    std::vector<int> updatedVec = increaseVector(vec, n);
    
    for(int num : updatedVec) {
        std::cout << num << " ";
    }
    
    return 0;
}
